<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import de.jensd.fx.glyphs.materialdesignicons.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AlunoController">

   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="5.0">
                <children>
                    <MaterialDesignIconView fx:id="btnIncluir" glyphName="PLUS" size="1.3em" />
                    <MaterialDesignIconView fx:id="btnAlterar" glyphName="PENCIL" size="1.3em" />
                    <MaterialDesignIconView fx:id="btnExcluir" glyphName="DELETE" size="1.3em" />
                </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>    
            </HBox>
            <TableView fx:id="tblView" prefHeight="350.0" prefWidth="310.0">
              <columns>
                <TableColumn prefWidth="75.0" text="CPF" >
                <cellValueFactory>
                    <PropertyValueFactory property="cpf"/>
                </cellValueFactory>
                </TableColumn>
                <TableColumn maxWidth="400.0" prefWidth="350.0" text="Nome" >
                <cellValueFactory>
                    <PropertyValueFactory property="nome"/>
                </cellValueFactory>
                </TableColumn>
              </columns>
            </TableView>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                <Button mnemonicParsing="false" text="Button" />
              </items>
            </ToolBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
