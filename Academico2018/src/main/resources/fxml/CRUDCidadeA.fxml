<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.materialdesignicons.*?>
<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="150.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CRUDCidadeAlunoController">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="150.0" prefWidth="250.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                        <VBox alignment="CENTER" spacing="5.0">
                            <children>
                                <Label text="Nome" />
                                <TextField fx:id="txtFldNome" maxWidth="200.0" prefHeight="25.0" prefWidth="200.0" />
                              <VBox alignment="CENTER">
                                  <children>
                                      <Label text="UF">
                                          <padding>
                                              <Insets bottom="5.0" />
                                          </padding>
                                      </Label>
                                      <ComboBox fx:id="cmbUf" prefWidth="200.0" />
                                  </children>
                              </VBox>
                            </children>
                        </VBox>
               </children>
            </VBox>
                <HBox alignment="TOP_CENTER" spacing="10.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#btnCancelaClick" prefWidth="100.0" text="Cancela">
                            <graphic>
                                <MaterialDesignIconView glyphName="CLOSE" />
                            </graphic>
                        </Button>
                        <Button fx:id="btnConfirma" mnemonicParsing="false" onAction="#btnConfirmaClick" prefWidth="100.0" text="Confirma">
                            <graphic>
                                <MaterialDesignIconView glyphName="CHECK" />
                            </graphic>
                        </Button>
                    </children>
                </HBox>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
